{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "server/**/*.ts"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/liabilities(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/app.ts"
    }
  ],
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs18.x"
    },
    "server/app.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
